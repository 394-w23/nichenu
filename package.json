{
  "name": "vite-template-react",
  "version": "1.2.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "cy:open": "cypress open --e2e",
    "em:start": "npm run build dev && firebase emulators:start --import=./saved-data --export-on-exit",
    "em:starttest": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:run": "firebase emulators:exec --import=./saved-data 'npm start'",
    "em:execui": "firebase emulators:exec --ui --import=./saved-data 'npm start'",
    "build": "vite build --mode prod",
    "build dev": "vite build --mode dev",
    "serve": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@mantine/core": "^5.10.0",
    "@mantine/dates": "^5.10.0",
    "@mantine/form": "^5.10.1",
    "@react-icons/all-files": "^4.1.0",
    "bootstrap": "^5.2.3",
    "firebase": "^9.15.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-uuid": "^2.0.0",
    "styled-components": "^5.3.6",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "c8": "^7.12.0",
    "cypress": "^12.4.1",
    "jsdom": "^20.0.3",
    "vite": "^4.0.0"
  }
}
